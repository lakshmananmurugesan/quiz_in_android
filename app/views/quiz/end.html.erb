<h1>Android Quiz Results</h1>
<p id="question">You answered correctly <%= @correct %> out of <%= @total %> questions.</p>

<p id="score"><%= @score %>%</p>

<%= form_tag({:controller => "quiz", :action => "index"}, :method => "get") do %>
  <div class="center">
	 <%= submit_tag("Back To Quiz", :class => "submit") %>
  </div>
<% end %>

<% content_for(:title) do %>
  Android Quiz Results
<% end %>